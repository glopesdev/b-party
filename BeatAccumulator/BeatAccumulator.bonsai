def process(value):
  x = value.Item1
  in_min = value.Item2
  in_max = value.Item3
  out_min = value.Item4
  out_max = value.Item5
  return (x - in_min) * (out_max - out_min) / (in_max - in_min) + out_min</q4:Script>
